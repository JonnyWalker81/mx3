@import "thread.djinni"
@import "studio.djinni"

studio_observer = interface +o +j {
	OnLogin();
	OnSessionsRetrieved(sessions: list<SessionsDto>);
}

studio_vm = interface +c {
	static createStudioClient(http_impl:http, launcher: thread_launcher, studioObserver: studio_observer): studio_vm;
    login(username: string, password: string);
    getSessions();
}
